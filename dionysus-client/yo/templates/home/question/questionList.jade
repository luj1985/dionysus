.ui.divided.items
  |{{#each questions}}
  .item
    .ui.small.image.createdby
      img(src="{{author.avatar}}")
    .content
      a.header(href="/questions/{{id}}") {{title}}
      .meta
        span.author {{author.username}}
        span.createdAt {{createdDate.year}}-{{createdDate.monthOfYear}}-{{createdDate.dayOfMonth}}
        span.numberAnswer {{numberOfAnswers}}
      .description
        p {{description}}
  |{{/each}}
#pagnation
